{
  "name": "Night Mode",
  "description": "Enable night mode in Chrome",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["nightMode.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Night Mode-Enabled"
  },
  "manifest_version": 2
}
